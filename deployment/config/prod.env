# Production environment variables

# Deployment configuration
DEPLOYMENT_TYPE=kubernetes  # kubernetes, docker-compose, ray-cluster
NAMESPACE=queue-engine-prod
API_HOST=queue-engine.example.com

# Ray configuration
RAY_ADDRESS=auto
RAY_CLUSTER_MODE=none
RAY_DASHBOARD_HOST=0.0.0.0
RAY_DASHBOARD_PORT=8265

# Application configuration
LOG_LEVEL=info
MIN_WORKERS=5
MAX_WORKERS=20
TASKS_PER_SECOND=100
MAX_CONCURRENT_TASKS=200